{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "status": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "photo": {
            "type": ["null", "string"]
          },
          "address": {
            "type": ["null", "string"]
          }
        },
        "required": ["id", "name", "photo"]
      }
    }
  },
  "required": ["status", "message", "data"]
}
